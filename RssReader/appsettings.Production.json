{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RssReaderContext": "Data Source=RssReader.db"
  },
  "FeedUpdate": {
    "IntervalMinutes": 60
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8099"
      }
    }
  }
}